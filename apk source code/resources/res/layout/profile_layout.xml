<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/profile_root"
    android:clickable="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <RelativeLayout
        android:orientation="vertical"
        android:id="@+id/profile_progress_container"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ProgressBar
            android:id="@+id/progress_spinner"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:indeterminate="true"
            android:layout_centerInParent="true"/>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/progress_cancel_btn"
            android:focusable="true"
            android:clickable="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="50dp"
            android:layout_below="@+id/progress_spinner"
            android:layout_centerHorizontal="true">
            <view.QuicksandTextView
                android:textSize="@dimen/dialog_text_small"
                android:textColor="@color/dialog_text_color"
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/cancel"/>
            <ImageView
                android:layout_gravity="center_horizontal"
                android:id="@+id/progess_cancel_btn_img"
                android:background="@color/alphabet_background_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/dialog_decline"/>
        </LinearLayout>
    </RelativeLayout>
    <LinearLayout
        android:gravity="center"
        android:orientation="vertical"
        android:id="@+id/profile_start_container"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <view.QuicksandTextView
            android:textSize="@dimen/text_large"
            android:gravity="center"
            android:id="@+id/profile_start_title"
            android:padding="8dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:text="@string/profile_start_title"/>
        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/profile_start_login_container"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <view.QuicksandTextView
                    android:textSize="@dimen/dialog_text_small"
                    android:textColor="@color/dialog_text_color"
                    android:gravity="center_vertical"
                    android:id="@+id/profile_start_login_label"
                    android:clickable="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/profile_start_login"
                    android:paddingStart="8dp"
                    android:paddingEnd="8dp"/>
                <ImageView
                    android:id="@+id/profile_start_login_button"
                    android:clickable="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/dialog_accept"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:id="@+id/profile_start_register_container"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/profile_start_login_container">
                <view.QuicksandTextView
                    android:textSize="@dimen/dialog_text_small"
                    android:textColor="@color/dialog_text_color"
                    android:gravity="center_vertical"
                    android:id="@+id/profile_start_register_label"
                    android:clickable="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/profile_start_register"
                    android:paddingStart="8dp"
                    android:paddingEnd="8dp"/>
                <ImageView
                    android:id="@+id/profile_start_register_button"
                    android:clickable="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/dialog_accept"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/profile_start_register_container">
                <view.QuicksandTextView
                    android:textSize="@dimen/dialog_text_small"
                    android:textColor="@color/dialog_text_color"
                    android:gravity="center_vertical"
                    android:id="@+id/profile_start_skip_label"
                    android:visibility="gone"
                    android:clickable="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/skip"
                    android:paddingStart="8dp"
                    android:paddingEnd="8dp"/>
                <ImageView
                    android:id="@+id/profile_start_skip_button"
                    android:visibility="gone"
                    android:clickable="true"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/dialog_decline"/>
            </LinearLayout>
        </RelativeLayout>
    </LinearLayout>
    <ScrollView
        android:id="@+id/profile_signup_container"
        android:visibility="gone"
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <RelativeLayout
            android:paddingBottom="200dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:id="@+id/profile_signup_step1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/profile_signup_title_container"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    app:hintEnabled="true">
                    <Spinner
                        android:entries="@array/titles"
                        android:id="@+id/profile_signup_title"
                        android:background="@drawable/shape_settings_rectangle"
                        android:padding="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/profile_signup_firstname_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:hint="@string/signup_firstname_hint"
                    android:layout_below="@+id/profile_signup_title_container"
                    app:hintEnabled="true">
                    <view.QuicksandTextInputEditText
                        android:id="@+id/profile_signup_firstname"
                        android:background="@drawable/shape_settings_rectangle"
                        android:padding="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textEmailAddress"/>
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/profile_signup_lastname_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:hint="@string/signup_lastname_hint"
                    android:layout_below="@+id/profile_signup_firstname_container"
                    app:hintEnabled="true"
                    app:passwordToggleEnabled="true">
                    <view.QuicksandTextInputEditText
                        android:id="@+id/profile_signup_lastname"
                        android:background="@drawable/shape_settings_rectangle"
                        android:padding="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="text"/>
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/profile_signup_address_container"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:hint="@string/signup_address_hint"
                    android:layout_below="@+id/profile_signup_lastname_container"
                    app:hintEnabled="true">
                    <view.QuicksandTextInputEditText
                        android:id="@+id/profile_signup_address"
                        android:background="@drawable/shape_settings_rectangle"
                        android:padding="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:maxLines="1"/>
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/profile_signup_address2_container"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:hint="@string/signup_address2_hint"
                    android:layout_below="@+id/profile_signup_address_container"
                    app:hintEnabled="true">
                    <view.QuicksandTextInputEditText
                        android:id="@+id/profile_signup_address2"
                        android:background="@drawable/shape_settings_rectangle"
                        android:padding="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:maxLines="1"/>
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/profile_signup_postalcode_container"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:hint="@string/signup_postalcode_hint"
                    android:layout_below="@+id/profile_signup_address2_container"
                    app:hintEnabled="true">
                    <view.QuicksandTextInputEditText
                        android:id="@+id/profile_signup_postalcode"
                        android:background="@drawable/shape_settings_rectangle"
                        android:padding="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:maxLines="1"/>
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/profile_signup_city_container"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:hint="@string/signup_city_hint"
                    android:layout_below="@+id/profile_signup_postalcode_container"
                    app:hintEnabled="true">
                    <view.QuicksandTextInputEditText
                        android:id="@+id/profile_signup_city"
                        android:background="@drawable/shape_settings_rectangle"
                        android:padding="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:maxLines="1"/>
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/profile_signup_country_container"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:hint="@string/signup_country_hint"
                    android:layout_below="@+id/profile_signup_city_container"
                    app:hintEnabled="true">
                    <Spinner
                        android:entries="@array/countries"
                        android:id="@+id/profile_signup_country"
                        android:background="@drawable/shape_settings_rectangle"
                        android:padding="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"/>
                </android.support.design.widget.TextInputLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="15dp"
                    android:layout_below="@+id/profile_signup_country_container">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true">
                        <view.QuicksandTextView
                            android:textSize="@dimen/dialog_text_small"
                            android:textColor="@color/dialog_text_color"
                            android:layout_gravity="center_horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tutorial_back"/>
                        <ImageView
                            android:layout_gravity="center_horizontal"
                            android:id="@+id/profile_signup_step1_back_btn"
                            android:background="@color/alphabet_background_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/dialog_decline"/>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true">
                        <view.QuicksandTextView
                            android:textSize="@dimen/dialog_text_small"
                            android:textColor="@color/dialog_text_color"
                            android:layout_gravity="center_horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tutorial_next"/>
                        <ImageView
                            android:layout_gravity="center_horizontal"
                            android:id="@+id/profile_signup_step1_next_btn"
                            android:background="@color/alphabet_background_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/dialog_next"/>
                    </LinearLayout>
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/profile_signup_step2"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <view.QuicksandTextView
                    android:textColor="@color/accent"
                    android:id="@+id/profile_signup_register_error"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"/>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/profile_signup_nickname_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:hint="@string/signup_nickname_hint"
                    app:hintEnabled="true">
                    <view.QuicksandTextInputEditText
                        android:id="@+id/profile_signup_nickname"
                        android:background="@drawable/shape_settings_rectangle"
                        android:padding="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textEmailAddress"/>
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/profile_signup_email_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:hint="@string/signup_email_hint"
                    android:layout_below="@+id/profile_signup_nickname_container"
                    app:hintEnabled="true">
                    <view.QuicksandTextInputEditText
                        android:id="@+id/profile_signup_email"
                        android:background="@drawable/shape_settings_rectangle"
                        android:padding="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textEmailAddress"/>
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/profile_signup_password_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:hint="@string/signup_password_hint"
                    android:layout_below="@+id/profile_signup_email_container"
                    app:hintEnabled="true"
                    app:passwordToggleEnabled="true">
                    <view.QuicksandTextInputEditText
                        android:id="@+id/profile_signup_password"
                        android:background="@drawable/shape_settings_rectangle"
                        android:padding="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textPassword"/>
                </android.support.design.widget.TextInputLayout>
                <android.support.design.widget.TextInputLayout
                    android:id="@+id/profile_signup_password2_container"
                    android:visibility="gone"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:hint="@string/signup_password2_hint"
                    android:layout_below="@+id/profile_signup_password_container"
                    app:hintEnabled="true"
                    app:passwordToggleEnabled="true">
                    <view.QuicksandTextInputEditText
                        android:id="@+id/profile_signup_password2"
                        android:background="@drawable/shape_settings_rectangle"
                        android:padding="10dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:inputType="textPassword"/>
                </android.support.design.widget.TextInputLayout>
                <LinearLayout
                    android:orientation="horizontal"
                    android:id="@+id/profile_signup_privacy_policy_container"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_margin="8dp"
                    android:layout_below="@+id/profile_signup_password_container">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <CheckBox
                            android:id="@+id/profile_signup_privacy_policy_cb"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/profile_signup_privacy_policy_tv"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"/>
                    </LinearLayout>
                </LinearLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginTop="20dp"
                    android:layout_marginRight="15dp"
                    android:layout_below="@+id/profile_signup_privacy_policy_container">
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentStart="true">
                        <view.QuicksandTextView
                            android:textSize="@dimen/dialog_text_small"
                            android:textColor="@color/dialog_text_color"
                            android:layout_gravity="center_horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tutorial_back"/>
                        <ImageView
                            android:layout_gravity="center_horizontal"
                            android:id="@+id/profile_signup_step2_back_btn"
                            android:background="@color/alphabet_background_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/dialog_decline"/>
                    </LinearLayout>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentEnd="true">
                        <view.QuicksandTextView
                            android:textSize="@dimen/dialog_text_small"
                            android:textColor="@color/dialog_text_color"
                            android:layout_gravity="center_horizontal"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/tutorial_next"/>
                        <ImageView
                            android:layout_gravity="center_horizontal"
                            android:id="@+id/profile_signup_submit"
                            android:background="@color/alphabet_background_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:src="@drawable/dialog_accept"/>
                    </LinearLayout>
                </RelativeLayout>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/profile_signup_done"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="400dp"
                android:layout_marginTop="10dp"
                android:layout_centerHorizontal="true">
                <view.QuicksandTextView
                    android:textSize="@dimen/dialog_text_small"
                    android:layout_width="500dp"
                    android:layout_height="wrap_content"
                    android:text="@string/profile_signup_done_text"
                    android:layout_centerHorizontal="true"/>
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_margin="15dp"
                    android:layout_alignParentBottom="true"
                    android:layout_alignParentEnd="true">
                    <view.QuicksandTextView
                        android:textSize="@dimen/dialog_text_small"
                        android:textColor="@color/dialog_text_color"
                        android:layout_gravity="center_horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/tutorial_next"/>
                    <ImageButton
                        android:id="@+id/profile_signup_to_signin_btn"
                        android:background="@color/alphabet_background_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:src="@drawable/dialog_next"/>
                </LinearLayout>
            </RelativeLayout>
        </RelativeLayout>
    </ScrollView>
    <RelativeLayout
        android:id="@+id/profile_login_container"
        android:visibility="gone"
        android:layout_width="700dp"
        android:layout_height="match_parent"
        android:layout_marginTop="50dp"
        android:layout_centerInParent="true">
        <view.QuicksandTextView
            android:textColor="@color/accent"
            android:gravity="center_horizontal"
            android:id="@+id/profile_login_error_text"
            android:paddingBottom="20dp"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=""/>
        <android.support.design.widget.TextInputLayout
            android:id="@+id/profile_login_email_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            app:hintEnabled="true">
            <view.QuicksandTextInputEditText
                android:id="@+id/profile_login_email"
                android:background="@drawable/shape_settings_rectangle"
                android:padding="7dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:hint="@string/signup_email_hint"
                android:inputType="textEmailAddress"/>
        </android.support.design.widget.TextInputLayout>
        <android.support.design.widget.TextInputLayout
            android:id="@+id/profile_login_password_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:layout_below="@+id/profile_login_email_container"
            app:hintEnabled="true"
            app:passwordToggleEnabled="true">
            <view.QuicksandTextInputEditText
                android:id="@+id/profile_login_password"
                android:background="@drawable/shape_settings_rectangle"
                android:padding="7dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text=""
                android:hint="@string/login_password_hint"
                android:inputType="textPassword"/>
        </android.support.design.widget.TextInputLayout>
        <LinearLayout
            android:gravity="center_horizontal"
            android:orientation="horizontal"
            android:id="@+id/dialog_buttons"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:baselineAligned="false"
            android:weightSum="2"
            android:layout_below="@+id/profile_login_password_container">
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/profile_login_back"
                android:focusable="true"
                android:clickable="true"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <view.QuicksandTextView
                    android:textSize="@dimen/dialog_text_small"
                    android:textColor="@color/dialog_text_color"
                    android:layout_gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/back"/>
                <ImageView
                    android:layout_gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/dialog_decline"/>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:id="@+id/profile_login_submit"
                android:focusable="true"
                android:clickable="true"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <view.QuicksandTextView
                    android:textSize="@dimen/dialog_text_small"
                    android:textColor="@color/dialog_text_color"
                    android:layout_gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/login"/>
                <ImageView
                    android:layout_gravity="center_horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/dialog_accept"/>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/profile_login_done_container"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="400dp"
        android:layout_marginTop="10dp"
        android:layout_centerHorizontal="true">
        <view.QuicksandTextView
            android:textSize="@dimen/dialog_text_small"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="100dp"
            android:layout_marginRight="100dp"
            android:text="@string/profile_login_done_text"
            android:layout_centerHorizontal="true"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15dp"
            android:layout_alignParentBottom="true"
            android:layout_alignParentEnd="true">
            <view.QuicksandTextView
                android:textSize="@dimen/dialog_text_small"
                android:textColor="@color/dialog_text_color"
                android:layout_gravity="center_horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/tutorial_next"/>
            <ImageButton
                android:id="@+id/profile_signin_to_info"
                android:background="@color/alphabet_background_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/dialog_next"/>
        </LinearLayout>
    </RelativeLayout>
    <RelativeLayout
        android:id="@+id/profile_userdetails_container"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <view.QuicksandTextView
            android:textSize="@dimen/text_small_middle"
            android:gravity="center"
            android:id="@+id/profile_userdetails_logout"
            android:background="@drawable/shape_settings_rectangle_rounded_right_top"
            android:padding="8dp"
            android:clickable="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="8dp"
            android:text="@string/logout"
            android:layout_centerHorizontal="true"
            android:textAllCaps="true"
            android:layout_alignParentEnd="true"
            style="@style/settings_right_row_tv"/>
        <LinearLayout
            android:orientation="vertical"
            android:id="@+id/profile_userdetails_info_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/profile_userdetails_logout"
            android:paddingStart="16dp"
            android:paddingEnd="16dp"
            android:layout_toStartOf="@+id/profile_userdetails_logout">
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="15dp"
                android:weightSum="10">
                <view.QuicksandTextView
                    android:textSize="@dimen/text_middle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/nickname"
                    android:layout_weight="3"
                    style="@style/settings_middle_row_tv"/>
                <view.QuicksandTextView
                    android:textSize="@dimen/text_middle"
                    android:id="@+id/profile_userdetails_nickname"
                    android:background="@drawable/shape_settings_rectangle_not_rounded"
                    android:paddingLeft="10dp"
                    android:paddingTop="5dp"
                    android:paddingRight="10dp"
                    android:paddingBottom="5dp"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="7"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="10">
                <view.QuicksandTextView
                    android:textSize="@dimen/text_middle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/email"
                    android:layout_weight="3"
                    style="@style/settings_middle_row_tv"/>
                <view.QuicksandTextView
                    android:textSize="@dimen/text_middle"
                    android:id="@+id/profile_userdetails_email"
                    android:background="@drawable/shape_settings_rectangle_not_rounded"
                    android:paddingLeft="10dp"
                    android:paddingTop="5dp"
                    android:paddingRight="10dp"
                    android:paddingBottom="5dp"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="7"/>
            </LinearLayout>
            <view.QuicksandTextView
                android:textSize="@dimen/text_large_middle"
                android:id="@+id/profile_userdetails_favorites"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="12dp"/>
        </LinearLayout>
    </RelativeLayout>
</RelativeLayout>
